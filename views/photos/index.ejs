<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head') %>
    <title>Photos</title>
</head>
<body>
    <main>
        <%- include('../partials/nav') %>
        <h1 class="title">All Photos</h1>
        <div class="photo-index-all">
            <% photos.forEach(photo=>{ %>
                <a href="/photos/<%= photo._id %>"><img class="user-photo-thumb" src="<%= photo.url %>" alt="<%= photo.about %>"></a>
                <% })%>
        </div>
    </main>
</body>
</html>